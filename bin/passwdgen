#!/usr/bin/env python

import random, string, sys

chars = filter(lambda c: c.isalnum(), map(chr, range(256)))
chars = chars * 2
chars += "!@#%^&*(){}[]/=?+_-;:,.<>"

def generate(length):
	return "".join([random.choice(chars) for i in xrange(length)])

if __name__ == "__main__":
	try:
		n = int(sys.argv[1])
	except:
		print "Usage: %s length" % sys.argv[0]
		sys.exit(1)

	print generate(n)




